<h1> Create New Patient</h1>


<form action="/clinician/dashboard" method="POST">
	<table>
		<tr>
			<td>
				First Name:
			</td>

			<td>
				<input type="text" placeholder="First name" name="first_name" />
			</td>
		</tr>
		<tr>
			<td>
				Last Name:
			</td>

			<td>
				<input type="text" placeholder="Last name" name="last_name" />
			</td>
		</tr>
		<tr>
			<td>
				Screen Name:
			</td>

			<td>
				<input type="text" placeholder="Screen Name" name="screen_name" />
			</td>
		</tr>
		<tr>
			<td>
				Date Of Birth:
			</td>

			<td>
				<input type="date" placeholder="DOB" name="dob" />
			</td>
		</tr>
		<tr>
			<td>
				BIO:
			</td>

			<td>
				<input type="date" placeholder="BIO" name="bio" />
			</td>
		</tr>
		<tr>
			<td>
				eng rate:
			</td>

			<td>
				<input type="text" placeholder="engagement rate" name="engagement_rate" />
			</td>
		</tr>
		<tr>
			<td>
				Email Address:
			</td>

			<td>
				<input type="text"  placeholder="email" name="email">
			</td>
		</tr>
        <tr>
            <td>
				Password:
			</td>

			<td>
				<input type="text" placeholder="password" name="password">
			</td>
		</tr>
	</table>

    <br>

    <table>
		<tr>
			<td>

			</td>

			<td>
				MIN
			</td>

            <td>
				MAX
			</td>
		</tr>
		<tr>
			<td>
				<input type="checkbox" id="recordBCG" name="recordBCG" value="Boolean">
                <label for="recordBCG"> BCG </label>
			</td>

			<td>
				<input type="text" placeholder="minimum" name="BCGmin" />
			</td>

            <td>
				<input type="text" placeholder="maxmimum" name="BCGmax" />
			</td>
		</tr>
        <tr>
			<td>
				<input type="checkbox" id="recordWeight" name="recordWeight" value="Weight">
                <label for="recordWeight"> Weight </label>
			</td>

			<td>
				<input type="text" placeholder="minimum" name="Weightmin" />
			</td>

            <td>
				<input type="text" placeholder="maxmimum" name="Weightmax" />
			</td>
		</tr>
        		<tr>
			<td>
				<input type="checkbox" id="recordInsulin" name="recordInsulin" value="Insulin">
                <label for="recordInsulin"> Insulin </label>
			</td>

			<td>
				<input type="text" placeholder="minimum" name="Insulinmin" />
			</td>

            <td>
				<input type="text" placeholder="maxmimum" name="Insulinmax" />
			</td>
		</tr>
        		<tr>
			<td>
				<input type="checkbox" id="recordExercise" name="recordExercise" value="Exercise">
                <label for="recordExercise"> Exercise </label>
			</td>

			<td>
				<input type="text" placeholder="minimum" name="Exercisemin" />
			</td>

            <td>
				<input type="text" placeholder="maxmimum" name="Exercisemax" />
			</td>
		</tr>

	</table>

    <input type="submit" />
</form> 
